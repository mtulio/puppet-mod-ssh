{
  "name": "mtulio-ssh",
  "version": "1.2.0",
  "author": "mtulio",
  "summary": "Module to manage SSH server and their config file.",
  "license": "Apache 2.0",
  "source": "https://github.com/mtulio/puppet-mod-ssh",
  "project_page": "https://github.com/mtulio/puppet-mod-ssh",
  "issues_url": "https://github.com/mtulio/puppet-mod-ssh/issues",
